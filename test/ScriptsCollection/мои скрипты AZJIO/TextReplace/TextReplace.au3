Func _GetFileList($sPath)
	Local $j, $tmp, $vFileList, $iEncoding
	$iEncoding = FileGetEncoding($sPath) ; îïðåäåëÿåì êîäèðîâêó
	$hFile = FileOpen($sPath, $iEncoding) ; Îòêðûâàåì â íóæíîé êîäèðîâêå äëÿ ïîääåðæêè êèòàéñêèõ òîâàðèùåé
	$vFileList = FileRead($hFile)
	FileClose($hFile)
	$vFileList=StringRegExpReplace($vFileList, '(?:[\r\n]+\h*)+', @CRLF) ; óäàëåíèå ïóñòûõ ñòðîê ïåðåä îáðàáîòêîé
	$vFileList=StringRegExpReplace($vFileList, '^[\r\n]+|[\r\n]+\z', '') ; óäàëåíèå ïóñòûõ ñòðîê â íà÷àëå è â êîíöå ñòðîêè
	$tmp = StringRegExp($vFileList, '(?mi)^(.+?)\r?$', 3)
	$tmp = UBound($tmp) ; êîëè÷åñòâî ñòðîê ñ äàííûìè â ôàéëå
	$vFileList = StringRegExp($vFileList, '(?mi)^([a-z]:\\.+?)\r?$', 3)
	If @error Then
		Return SetError(1) ; ñïèñîê ïóñò
	Else
		Local $aFileList[UBound($vFileList) + 1]
		$j = 0
		For $i = 0 To UBound($vFileList) - 1
			If FileExists($vFileList[$i]) Then
				$j += 1
				$aFileList[$j] = $vFileList[$i]
			EndIf
		Next
		If $j = 0 Then Return SetError(2) ; íåò ñóùåñòâóþùèõ ôàéëîâ
		$aFileList[0] = $j
		ReDim $aFileList[$j + 1]
		Return SetError(0, $tmp, $aFileList) ; Âîçâðàùàåì ñïèñîê ôàéëîâ
	EndIf
EndFunc   ;==>_GetFileList

