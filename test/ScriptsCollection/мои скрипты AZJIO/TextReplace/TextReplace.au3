	$Path = $aScenario[0][6]
	$Mask = $aScenario[0][7]
	$Include = $aScenario[0][8]
	$Depth = $aScenario[0][9]
	$ReData = $aScenario[0][10]
	$ExcldAttrib = $aScenario[0][11]
	$CharSet = _CharNameToNum($aScenario[0][12])
	If $CharSet = -1 Then
		$CharSetAuto = 1
	Else
