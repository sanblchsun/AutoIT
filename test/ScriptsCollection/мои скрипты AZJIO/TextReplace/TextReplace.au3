			$BackUpPath = $tmp
		Else
			If MsgBox(4, $LngErr, $LngMB17, 0, $hGui) = 7 Then
				_Enable()
				Return
			EndIf
		EndIf
		$BackUpPath &= @YEAR & "." & @MON & "." & @MDAY & "_" & @HOUR & "." & @MIN & "." & @SEC & "_" & StringRegExpReplace($PatInp0, '^(.*\\)(.*?\\)$', '\2')
	EndIf

	; çàñåêàåì âðåìÿ
	$timer = TimerInit()
	If $g_iTrPathDir Then
		$Depth = 0
