Func _ReplaceOnce()
	Local $tmpData, $TrR, $TrH, $TrS, $aExcAttrib, $file, $Size2, $iTotalRep, $extended
	$sOutList = ''
	ToolTip('')
	GUICtrlSetState($SeaBtn, $GUI_DISABLE)
	GUICtrlSetState($RepBtn, $GUI_DISABLE)
	GUICtrlSetData($StatusBar, '')
	$SeaInp0 = GUICtrlRead($SeaInp)
	$RepInp0 = GUICtrlRead($RepInp)
	$TypInp0 = GUICtrlRead($TypInp)
	$PatInp0 = GUICtrlRead($PatInp)
	$CRLF0 = GUICtrlRead($CRLF)
	If $CharSet = -1 Then
		$CharSetAuto = 1
	Else
		$CharSetAuto = 0
	EndIf

	; ïðîâåðêè-çàùèòû
	If $SeaInp0 == $RepInp0 And (StringRight($RepInp0, 4) <> '.srt' Or Not FileExists($RepInp0)) Then ; îøèáêà âõîäíûõ äàííûõ
		MsgBox(0, $LngErr, $LngMB3, 0, $hGui)
		_Enable()
		Return
	EndIf
	If $SeaInp0 = $RepInp0 And StringRight($RepInp0, 4) = '.srt' And FileExists($RepInp0) Then ; åñëè ñöåíàðèé, òî
		If IsArray($aTextScr) Then ; åñëè ñöåíàðèé íå ïóñòîé
			Return _ReplaceLoop($RepInp0) ; âûçîâ îáðàáîòêè ñöåíàðèÿ
		Else
			; $TrScr = 0
			MsgBox(0, $LngErr, $LngMB4, 0, $hGui)
			_Enable()
			Return
		EndIf
	EndIf

	$tmp = _FO_IsDir($PatInp0)
	If Not (@error Or $tmp) Then ; ïðîâåðêà, ÷òî óêàçàí ñïèñîê ôàéëîâ èëè ïàïêà
		$FileList = _GetFileList($PatInp0)
		If @error Then
			MsgBox(0, $LngErr, $LngMB1, 0, $hGui)
