Func _CharNumToName($i)
	If $CharSetAuto Then Return 'Auto'
	Switch $i
		Case 0
			$i = 'ANSI'
		Case 16
			$i = 'Bin'
		Case 32
			$i = 'UTF16 Little Endian'
		Case 64
			$i = 'UTF16 Big Endian'
		Case 128
			$i = 'UTF8 (+ BOM)'
		Case 256
			$i = 'UTF8 (- BOM)'
		Case Else
			$i = 'Auto'
	EndSwitch
	Return $i
EndFunc   ;==>_CharNumToName

Func _CharNameToNum($i)
	If $i = 'Auto' Then
		$CharSetAuto = 1
	Else
		$CharSetAuto = 0
	EndIf
	Switch $i
		Case 'Auto'
			$i = -1
		Case 'ANSI'
			$i = 0
		Case 'Bin'
			$i = 16
		Case 'UTF16 Little Endian'
			$i = 32
		Case 'UTF16 Big Endian'
			$i = 64
		Case 'UTF8 (+ BOM)'
			$i = 128
		Case 'UTF8 (- BOM)'
			$i = 256
		Case Else
			$i = -1
	EndSwitch
	Return $i
EndFunc   ;==>_CharNameToNum

Func _SetLang()
	Local $aLng
	$LangPath = GUICtrlRead($ComboLang)
