	EndIf
	
	; çàñåêàåì âðåìÿ
	$timer = TimerInit()
	$Depth = 0
	If $inisub = 1 Then $Depth = 125
	If $Include Then
		$Include = True
	Else
		$Include = False
	EndIf
	$FileList = _FO_FileSearch($Path, _FO_CorrectMask($Mask), $Include, $Depth, 1, 1, 1, $sLocale) ; ïîëó÷àåì ñïèñîê ôàéëîâ
	If @error Then

		$timer = TimerDiff($timer)
		If $timer < 9500 Then
