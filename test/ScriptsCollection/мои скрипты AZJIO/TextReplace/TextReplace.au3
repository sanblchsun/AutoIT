	$LenPath = StringLen($Path)
	
	; âûïîëíÿåì ïîèñê â öèêëå äëÿ âñåõ òèïîâ ôàéëîâ
	$Size1 = 0
	$kol = 0
	$ExcldAttrib = StringRegExpReplace($ExcldAttrib, '(?i)[^RASHNOT]', '') ; óäàëåíèå èç íàáîðà àòðèáóòîâ "ëåâûõ" ñèìâîëîâ
	$aExcAttrib = StringSplit($ExcldAttrib, '')
	HotKeySet("{ESC}", "_StopLoop")
	For $i = 1 To $FileList[0]
