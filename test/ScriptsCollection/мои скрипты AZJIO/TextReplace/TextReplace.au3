Func _EditorTextFound()
	$Out0 = GUICtrlRead($Out)
	If Not $Out0 Then Return ; âûäåëèòå ôàéë
	$Out0 = StringLeft($Out0, StringInStr($Out0, '*') - 2)
	; $Out0=StringRegExpReplace($Out0, '(^.*)( \*\d+)$', '\1')
	If StringInStr($Out0, ':') Then $PatInp0 = ''
	Run($Editor & ' ' & $PatInp0 & $Out0)
	
	If $hf <> '' And Not $iniREx Then
		Sleep(100)
		$nameEditor = StringRegExpReplace($Editor, '(^.*)\\(.*)$', '\2')
		$AllWindows = WinList()
		For $i = 1 To $AllWindows[0][0]
			; åñëè îêíî èìååò òåêñò çàãîëîâêà, âèäèìîå è ïóòü îêíà ñîâïàäàåò ñ ïóò¸ì ðåäàêòîðà
			If $AllWindows[$i][0] And BitAND(WinGetState($AllWindows[$i][1]), 2) And StringRight(_ProcessGetPath(WinGetProcess($AllWindows[$i][1])), StringLen($nameEditor) + 1) = '\' & $nameEditor Then
				WinActivate($AllWindows[$i][1])
				If WinWaitActive($AllWindows[$i][1], '', 3) Then ; îæèäàåì 3 ñåê àêòèâíîñòü îêíà
					Sleep(30)
					Send('^{' & $hf & '}')
					Sleep(30)
					ClipPut($SeaInp0)
					Sleep(30)
					If WinWaitNotActive($AllWindows[$i][1], "", 1) And StringRight(_ProcessGetPath(WinGetProcess('[ACTIVE]')), StringLen($nameEditor) + 1) = '\' & $nameEditor Then ; åñëè àêòèâåí äèàëîã ïîèñêà, à íå îêíî ðåäàêòîðà, òî äåëàåì âñòàâêó
						Send('+{INS}')
						Sleep(30)
						Send('{Enter}')
					EndIf
					ExitLoop
				EndIf
			EndIf
		Next
	EndIf
EndFunc   ;==>_EditorTextFound

