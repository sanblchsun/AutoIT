font.base=font:Courier New,size:10,$(font.override)
font.monospace=font:Courier New,size:10
proper.case=0
check.updates.scite4autoit3=0
use.tabs=1
indent.size=4
indent.size.*.au3=4
tabsize=4
#Background
style.au3.32=style.*.32=$(font.base),back:#293134
# Brace highlight
style.au3.34=fore:#F3DB2E,back:#293134
# Brace incomplete highlight
style.au3.35=fore:#66747B,italics,back:#293134

# Line Number Margin - колонка номеров строк
style.au3.33=fore:#81969A,back:#3F4B4E,$(font.base)
style.au3.37=fore:#81969A,back:#3F4B4E
#line.margin.visible=1
line.margin.width=1+

#  олонка плюсиков дл€ разворачивани€ блоков
fold.margin.colour=#343F41
fold.margin.highlight.colour=#343F41
#fold.compact=0

# ћигающий курсор
caret.fore=#C1CBD2
caret.line.back=#2F393C
#caret.width=2
#caret.line.back.alpha=100

# ¬ыделенна€ область курсором
selection.fore=#EC7600
selection.back=#404E51
# selection.alpha=75

# ¬ертикальна€ лини€ справа, edge.mode=0 убирает эту линию, edge.column - отступ слева до линии
edge.colour=#81969A
edge.mode=0
#edge.column=500

#Console - запись в консоль при выполнении скрипта по F5
style.errorlist.32=back:#293134,$(font.monospace)
