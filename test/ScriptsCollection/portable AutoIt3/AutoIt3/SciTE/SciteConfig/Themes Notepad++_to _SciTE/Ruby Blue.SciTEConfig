font.base=font:Courier New,size:10,$(font.override)
font.monospace=font:Courier New,size:10
proper.case=0
check.updates.scite4autoit3=0
use.tabs=1
indent.size=4
indent.size.*.au3=4
tabsize=4
#Background
style.au3.32=style.*.32=$(font.base),back:#112435
# Brace highlight
style.au3.34=fore:#FFFFFF,back:#112435
# Brace incomplete highlight
style.au3.35=fore:#FFFFFF,italics,back:#112435

# Line Number Margin - колонка номеров строк
style.au3.33=fore:#FFFFFF,back:#1F4661,$(font.base)
style.au3.37=fore:#FFFFFF,back:#1F4661
#line.margin.visible=1
line.margin.width=1+

#  олонка плюсиков дл€ разворачивани€ блоков
fold.margin.colour=#4096BF
fold.margin.highlight.colour=#3476A3
#fold.compact=0

# ћигающий курсор
caret.fore=#FFFFFF
caret.line.back=#273A4B
#caret.width=2
#caret.line.back.alpha=100

# ¬ыделенна€ область курсором
selection.fore=#FFFFFF
selection.back=#0000FF
# selection.alpha=75

# ¬ертикальна€ лини€ справа, edge.mode=0 убирает эту линию, edge.column - отступ слева до линии
edge.colour=#FFFFFF
edge.mode=0
#edge.column=500

#Console - запись в консоль при выполнении скрипта по F5
style.errorlist.32=back:#112435,$(font.monospace)
style.errorlist.0=fore:#FFFFFF
style.errorlist.5=fore:#000000
style.errorlist.3=fore:#FFFFFF
style.errorlist.4=fore:#FFFFFF
style.errorlist.11=fore:#FFFFFF
style.errorlist.12=fore:#0
error.marker.fore=fore:#ff0000,italics,back:#112435
colour.error=fore:#ff0000,back:#FFFFFF
#White space
style.au3.0=fore:#FFFFFF,back:#112435
#Comment line
style.au3.1=fore:#FFFFFF,italics,back:#112435
#Comment block
style.au3.2=fore:#FFFFFF,italics,back:#112435
#Number
style.au3.3=fore:#FFFFFF,back:#112435
#Function
style.au3.4=fore:#FFFFFF,back:#112435
#Keyword
style.au3.5=fore:#FFFFFF,back:#112435
#Macro
style.au3.6=fore:#FFFFFF,back:#112435
#String
style.au3.7=fore:#FFFFFF,back:#112435
#Operator
style.au3.8=fore:#FFFFFF,back:#112435
#Variable
style.au3.9=fore:#8DB0D3,back:#112435
#Sent keys
style.au3.10=fore:#0,bold,back:#112435
#Pre-Processor
style.au3.11=fore:#FFFFFF,back:#112435
#Special
style.au3.12=fore:#FFFFFF,back:#112435
#Abbrev-Expand
style.au3.13=fore:#0,bold,back:#112435
#Com Objects
style.au3.14=fore:#0,bold,back:#112435
#Standard UDF's
style.au3.15=fore:#FFFFFF,back:#112435