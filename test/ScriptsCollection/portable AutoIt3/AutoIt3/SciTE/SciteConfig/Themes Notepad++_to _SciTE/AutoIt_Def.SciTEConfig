font.base=font:Arial,size:10,$(font.override)
font.monospace=font:Arial,size:10
proper.case=0
check.updates.scite4autoit3=0
use.tabs=1
indent.size=4
indent.size.*.au3=4
tabsize=4
#Background
style.au3.32=style.*.32=$(font.base),back:#F0F4F9
# Brace highlight
style.au3.34=fore:#FF0000,back:#F0F4F9
# Brace incomplete highlight
style.au3.35=fore:#008000,italics,back:#F0F4F9

# Line Number Margin - колонка номеров строк
style.au3.33=fore:#808080,back:#E4E4E4,$(font.base)
style.au3.37=fore:#808080,back:#E4E4E4
#line.margin.visible=1
line.margin.width=1+

#  олонка плюсиков дл€ разворачивани€ блоков
fold.margin.colour=#FFFFFF
fold.margin.highlight.colour=#E9E9E9
#fold.compact=0

# ћигающий курсор
caret.fore=#8000FF
caret.line.back=#FFFED8
#caret.width=2
#caret.line.back.alpha=100

# ¬ыделенна€ область курсором
selection.fore=#9999DF
selection.back=#E0E0E8
# selection.alpha=75

# ¬ертикальна€ лини€ справа, edge.mode=0 убирает эту линию, edge.column - отступ слева до линии
edge.colour=#808080
edge.mode=0
#edge.column=500

#Console - запись в консоль при выполнении скрипта по F5
style.errorlist.32=back:#F0F4F9,$(font.monospace)
style.errorlist.0=fore:#9999DF
style.errorlist.5=fore:#000000
style.errorlist.3=fore:#FF0000
style.errorlist.4=fore:#000090
style.errorlist.11=fore:#008000
style.errorlist.12=fore:#FF8800
error.marker.fore=fore:#ff0000,italics,back:#F0F4F9
colour.error=fore:#ff0000,back:#FF0000
#White space
style.au3.0=fore:#004080,back:#F0F4F9
#Comment line
style.au3.1=fore:#008000,italics,back:#F0F4F9
#Comment block
style.au3.2=fore:#008000,italics,back:#F0F4F9
#Number
style.au3.3=fore:#9F009F,back:#F0F4F9
#Function
style.au3.4=fore:#000090,back:#F0F4F9
#Keyword
style.au3.5=fore:#0000FF,back:#F0F4F9
#Macro
style.au3.6=fore:#FF00FF,back:#F0F4F9
#String
style.au3.7=fore:#9999DF,back:#F0F4F9
#Operator
style.au3.8=fore:#FF0000,back:#F0F4F9
#Variable
style.au3.9=fore:#AA0000,back:#F0F4F9
#Sent keys
style.au3.10=fore:#FF8800,bold,back:#F0F4F9
#Pre-Processor
style.au3.11=fore:#F000FF,back:#F0F4F9
#Special
style.au3.12=fore:#000000,back:#F0F4F9
#Abbrev-Expand
style.au3.13=fore:#000000,bold,back:#F0F4F9
#Com Objects
style.au3.14=fore:#000000,bold,back:#F0F4F9
#Standard UDF's
style.au3.15=fore:#004080,back:#F0F4F9