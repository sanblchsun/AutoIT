font.base=font:DejaVu Sans Mono,size:10,$(font.override)
font.monospace=font:DejaVu Sans Mono,size:10
proper.case=0
check.updates.scite4autoit3=0
use.tabs=1
indent.size=4
indent.size.*.au3=4
tabsize=4
#Background
style.au3.32=style.*.32=$(font.base),back:#0C1021
# Brace highlight
style.au3.34=fore:#FCE94F,back:#0C1021
# Brace incomplete highlight
style.au3.35=fore:#AEAEAE,italics,back:#0C1021

# Line Number Margin - колонка номеров строк
style.au3.33=fore:#EEEEEC,back:#2E3436,$(font.base)
style.au3.37=fore:#EEEEEC,back:#2E3436
#line.margin.visible=1
line.margin.width=1+

#  олонка плюсиков дл€ разворачивани€ блоков
fold.margin.colour=#555753
fold.margin.highlight.colour=#2E3436
#fold.compact=0

# ћигающий курсор
caret.fore=#FFFFFF
caret.line.back=#121830
#caret.width=2
#caret.line.back.alpha=100

# ¬ыделенна€ область курсором
selection.fore=#61CE3C
selection.back=#253B76
# selection.alpha=75

# ¬ертикальна€ лини€ справа, edge.mode=0 убирает эту линию, edge.column - отступ слева до линии
edge.colour=#EEEEEC
edge.mode=0
#edge.column=500

#Console - запись в консоль при выполнении скрипта по F5
style.errorlist.32=back:#0C1021,$(font.monospace)
style.errorlist.0=fore:#61CE3C
style.errorlist.5=fore:#000000
style.errorlist.3=fore:#FBDE2D
style.errorlist.4=fore:#8DA6CE
style.errorlist.11=fore:#AEAEAE
style.errorlist.12=fore:#000000
error.marker.fore=fore:#ff0000,italics,back:#0C1021
colour.error=fore:#ff0000,back:#FBDE2D
#White space
style.au3.0=fore:#F8F8F8,back:#0C1021
#Comment line
style.au3.1=fore:#AEAEAE,italics,back:#0C1021
#Comment block
style.au3.2=fore:#AEAEAE,italics,back:#0C1021
#Number
style.au3.3=fore:#D8FA3C,back:#0C1021
#Function
style.au3.4=fore:#8DA6CE,back:#0C1021
#Keyword
style.au3.5=fore:#FBDE2D,back:#0C1021
#Macro
style.au3.6=fore:#000000,back:#0C1021
#String
style.au3.7=fore:#61CE3C,back:#0C1021
#Operator
style.au3.8=fore:#FBDE2D,back:#0C1021
#Variable
style.au3.9=fore:#F8F8F8,back:#0C1021
#Sent keys
style.au3.10=fore:#000000,bold,back:#0C1021
#Pre-Processor
style.au3.11=fore:#FBDE2D,back:#0C1021
#Special
style.au3.12=fore:#000000,back:#0C1021
#Abbrev-Expand
style.au3.13=fore:#000000,bold,back:#0C1021
#Com Objects
style.au3.14=fore:#000000,bold,back:#0C1021
#Standard UDF's
style.au3.15=fore:#F8F8F8,back:#0C1021