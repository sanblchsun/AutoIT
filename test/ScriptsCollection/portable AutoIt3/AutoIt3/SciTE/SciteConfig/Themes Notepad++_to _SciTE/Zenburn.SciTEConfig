font.base=font:Consolas,size:10,$(font.override)
font.monospace=font:Consolas,size:10
proper.case=0
check.updates.scite4autoit3=0
use.tabs=1
indent.size=4
indent.size.*.au3=4
tabsize=4
#Background
style.au3.32=style.*.32=$(font.base),back:#3F3F3F
# Brace highlight
style.au3.34=fore:#F0F9F9,back:#3F3F3F
# Brace incomplete highlight
style.au3.35=fore:#7F9F7F,italics,back:#3F3F3F

# Line Number Margin - колонка номеров строк
style.au3.33=fore:#8A8A8A,back:#0C0C0C,$(font.base)
style.au3.37=fore:#8A8A8A,back:#0C0C0C
#line.margin.visible=1
line.margin.width=1+

#  олонка плюсиков дл€ разворачивани€ блоков
fold.margin.colour=#181818
fold.margin.highlight.colour=#101010
#fold.compact=0

# ћигающий курсор
caret.fore=#8FAF9F
caret.line.back=#101010
#caret.width=2
#caret.line.back.alpha=100

# ¬ыделенна€ область курсором
selection.fore=#CC9393
selection.back=#585858
# selection.alpha=75

# ¬ертикальна€ лини€ справа, edge.mode=0 убирает эту линию, edge.column - отступ слева до линии
edge.colour=#8A8A8A
edge.mode=0
#edge.column=500

#Console - запись в консоль при выполнении скрипта по F5
style.errorlist.32=back:#3F3F3F,$(font.monospace)
style.errorlist.0=fore:#CC9393
style.errorlist.5=fore:#000000
style.errorlist.3=fore:#9F9D6D
style.errorlist.4=fore:#EFEF8F
style.errorlist.11=fore:#7F9F7F
style.errorlist.12=fore:#DCDCCC
error.marker.fore=fore:#ff0000,italics,back:#3F3F3F
colour.error=fore:#ff0000,back:#9F9D6D
#White space
style.au3.0=fore:#DCDCCC,back:#3F3F3F
#Comment line
style.au3.1=fore:#7F9F7F,italics,back:#3F3F3F
#Comment block
style.au3.2=fore:#7F9F7F,italics,back:#3F3F3F
#Number
style.au3.3=fore:#8CD0D3,back:#3F3F3F
#Function
style.au3.4=fore:#EFEF8F,back:#3F3F3F
#Keyword
style.au3.5=fore:#DFC47D,back:#3F3F3F
#Macro
style.au3.6=fore:#FFEBDD,back:#3F3F3F
#String
style.au3.7=fore:#CC9393,back:#3F3F3F
#Operator
style.au3.8=fore:#9F9D6D,back:#3F3F3F
#Variable
style.au3.9=fore:#DCDCCC,back:#3F3F3F
#Sent keys
style.au3.10=fore:#DCDCCC,bold,back:#3F3F3F
#Pre-Processor
style.au3.11=fore:#FFCFAF,back:#3F3F3F
#Special
style.au3.12=fore:#FFEBDD,back:#3F3F3F
#Abbrev-Expand
style.au3.13=fore:#CEDF99,bold,back:#3F3F3F
#Com Objects
style.au3.14=fore:#DCDCCC,bold,back:#3F3F3F
#Standard UDF's
style.au3.15=fore:#DCDCCC,back:#3F3F3F