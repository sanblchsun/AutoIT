font.base=font:DejaVu Sans Mono,size:10,$(font.override)
font.monospace=font:DejaVu Sans Mono,size:10
proper.case=0
check.updates.scite4autoit3=0
use.tabs=1
indent.size=4
indent.size.*.au3=4
tabsize=4
#Background
style.au3.32=style.*.32=$(font.base),back:#1A0F0B
# Brace highlight
style.au3.34=fore:#FCE94F,back:#1A0F0B
# Brace incomplete highlight
style.au3.35=fore:#679D47,italics,back:#1A0F0B

# Line Number Margin - колонка номеров строк
style.au3.33=fore:#EEEEEC,back:#2E3436,$(font.base)
style.au3.37=fore:#EEEEEC,back:#2E3436
#line.margin.visible=1
line.margin.width=1+

#  олонка плюсиков дл€ разворачивани€ блоков
fold.margin.colour=#555753
fold.margin.highlight.colour=#2E3436
#fold.compact=0

# ћигающий курсор
caret.fore=#A7A7A7
caret.line.back=#281711
#caret.width=2
#caret.line.back.alpha=100

# ¬ыделенна€ область курсором
selection.fore=#7CA563
selection.back=#372017
# selection.alpha=75

# ¬ертикальна€ лини€ справа, edge.mode=0 убирает эту линию, edge.column - отступ слева до линии
edge.colour=#EEEEEC
edge.mode=0
#edge.column=500

#Console - запись в консоль при выполнении скрипта по F5
style.errorlist.32=back:#1A0F0B,$(font.monospace)
style.errorlist.0=fore:#7CA563
style.errorlist.5=fore:#000000
style.errorlist.3=fore:#B3935C
style.errorlist.4=fore:#C29863
style.errorlist.11=fore:#679D47
style.errorlist.12=fore:#000000
error.marker.fore=fore:#ff0000,italics,back:#1A0F0B
colour.error=fore:#ff0000,back:#B3935C
#White space
style.au3.0=fore:#C3BE98,back:#1A0F0B
#Comment line
style.au3.1=fore:#679D47,italics,back:#1A0F0B
#Comment block
style.au3.2=fore:#679D47,italics,back:#1A0F0B
#Number
style.au3.3=fore:#DA5659,back:#1A0F0B
#Function
style.au3.4=fore:#C29863,back:#1A0F0B
#Keyword
style.au3.5=fore:#F1E694,back:#1A0F0B
#Macro
style.au3.6=fore:#000000,back:#1A0F0B
#String
style.au3.7=fore:#7CA563,back:#1A0F0B
#Operator
style.au3.8=fore:#B3935C,back:#1A0F0B
#Variable
style.au3.9=fore:#7989A6,back:#1A0F0B
#Sent keys
style.au3.10=fore:#000000,bold,back:#1A0F0B
#Pre-Processor
style.au3.11=fore:#8996A8,back:#1A0F0B
#Special
style.au3.12=fore:#000000,back:#1A0F0B
#Abbrev-Expand
style.au3.13=fore:#000000,bold,back:#1A0F0B
#Com Objects
style.au3.14=fore:#000000,bold,back:#1A0F0B
#Standard UDF's
style.au3.15=fore:#C3BE98,back:#1A0F0B