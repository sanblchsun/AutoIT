font.base=font:DejaVu Sans Mono,size:10,$(font.override)
font.monospace=font:DejaVu Sans Mono,size:10
proper.case=0
check.updates.scite4autoit3=0
use.tabs=1
indent.size=4
indent.size.*.au3=4
tabsize=4
#Background
style.au3.32=style.*.32=$(font.base),back:#2A211C
# Brace highlight
style.au3.34=fore:#E5C138,back:#2A211C
# Brace incomplete highlight
style.au3.35=fore:#1E9AE0,italics,back:#2A211C

# Line Number Margin - колонка номеров строк
style.au3.33=fore:#E5C138,back:#4C4A41,$(font.base)
style.au3.37=fore:#E5C138,back:#4C4A41
#line.margin.visible=1
line.margin.width=1+

#  олонка плюсиков дл€ разворачивани€ блоков
fold.margin.colour=#2E3436
fold.margin.highlight.colour=#6A5448
#fold.compact=0

# ћигающий курсор
caret.fore=#37A8ED
caret.line.back=#4B3C34
#caret.width=2
#caret.line.back.alpha=100

# ¬ыделенна€ область курсором
selection.fore=#55E439
selection.back=#83675A
# selection.alpha=75

# ¬ертикальна€ лини€ справа, edge.mode=0 убирает эту линию, edge.column - отступ слева до линии
edge.colour=#E5C138
edge.mode=0
#edge.column=500

#Console - запись в консоль при выполнении скрипта по F5
style.errorlist.32=back:#2A211C,$(font.monospace)
style.errorlist.0=fore:#55E439
style.errorlist.5=fore:#000000
style.errorlist.3=fore:#FFAA00
style.errorlist.4=fore:#FFB454
style.errorlist.11=fore:#1E9AE0
style.errorlist.12=fore:#000000
error.marker.fore=fore:#ff0000,italics,back:#2A211C
colour.error=fore:#ff0000,back:#FFAA00
#White space
style.au3.0=fore:#F8F8F8,back:#2A211C
#Comment line
style.au3.1=fore:#1E9AE0,italics,back:#2A211C
#Comment block
style.au3.2=fore:#1E9AE0,italics,back:#2A211C
#Number
style.au3.3=fore:#FF3A83,back:#2A211C
#Function
style.au3.4=fore:#FFB454,back:#2A211C
#Keyword
style.au3.5=fore:#F6F080,back:#2A211C
#Macro
style.au3.6=fore:#000000,back:#2A211C
#String
style.au3.7=fore:#55E439,back:#2A211C
#Operator
style.au3.8=fore:#FFAA00,back:#2A211C
#Variable
style.au3.9=fore:#FB9A4B,back:#2A211C
#Sent keys
style.au3.10=fore:#000000,bold,back:#2A211C
#Pre-Processor
style.au3.11=fore:#FFAA00,back:#2A211C
#Special
style.au3.12=fore:#000000,back:#2A211C
#Abbrev-Expand
style.au3.13=fore:#000000,bold,back:#2A211C
#Com Objects
style.au3.14=fore:#000000,bold,back:#2A211C
#Standard UDF's
style.au3.15=fore:#F8F8F8,back:#2A211C