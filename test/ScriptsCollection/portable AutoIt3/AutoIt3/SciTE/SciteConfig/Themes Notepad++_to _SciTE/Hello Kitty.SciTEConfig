font.base=font:Courier New,size:10,$(font.override)
font.monospace=font:Courier New,size:10
proper.case=0
check.updates.scite4autoit3=0
use.tabs=1
indent.size=4
indent.size.*.au3=4
tabsize=4
#Background
style.au3.32=style.*.32=$(font.base),back:#FFB0FF
# Brace highlight
style.au3.34=fore:#FF0000,back:#FFB0FF
# Brace incomplete highlight
style.au3.35=fore:#008000,italics,back:#FFB0FF

# Line Number Margin - колонка номеров строк
style.au3.33=fore:#FFFFFF,back:#FF80FF,$(font.base)
style.au3.37=fore:#FFFFFF,back:#FF80FF
#line.margin.visible=1
line.margin.width=1+

#  олонка плюсиков дл€ разворачивани€ блоков
fold.margin.colour=#FF80C0
fold.margin.highlight.colour=#FF80C0
#fold.compact=0

# ћигающий курсор
caret.fore=#FFFFFF
caret.line.back=#FF80C0
#caret.width=2
#caret.line.back.alpha=100

# ¬ыделенна€ область курсором
selection.fore=#808080
selection.back=#FFD5FF
# selection.alpha=75

# ¬ертикальна€ лини€ справа, edge.mode=0 убирает эту линию, edge.column - отступ слева до линии
edge.colour=#FFFFFF
edge.mode=0
#edge.column=500

#Console - запись в консоль при выполнении скрипта по F5
style.errorlist.32=back:#FFB0FF,$(font.monospace)
style.errorlist.0=fore:#808080
style.errorlist.5=fore:#000000
style.errorlist.3=fore:#000080
style.errorlist.4=fore:#8000FF
style.errorlist.11=fore:#008000
style.errorlist.12=fore:#8080C0
error.marker.fore=fore:#ff0000,italics,back:#FFB0FF
colour.error=fore:#ff0000,back:#000080
#White space
style.au3.0=fore:#000000,back:#FFB0FF
#Comment line
style.au3.1=fore:#008000,italics,back:#FFB0FF
#Comment block
style.au3.2=fore:#008000,italics,back:#FFB0FF
#Number
style.au3.3=fore:#FF8000,back:#FFB0FF
#Function
style.au3.4=fore:#8000FF,back:#FFB0FF
#Keyword
style.au3.5=fore:#0000FF,back:#FFB0FF
#Macro
style.au3.6=fore:#8000FF,back:#FFB0FF
#String
style.au3.7=fore:#808080,back:#FFB0FF
#Operator
style.au3.8=fore:#000080,back:#FFB0FF
#Variable
style.au3.9=fore:#FF0080,back:#FFB0FF
#Sent keys
style.au3.10=fore:#8080C0,bold,back:#FFB0FF
#Pre-Processor
style.au3.11=fore:#804000,back:#FFB0FF
#Special
style.au3.12=fore:#000000,back:#FFB0FF
#Abbrev-Expand
style.au3.13=fore:#000000,bold,back:#FFB0FF
#Com Objects
style.au3.14=fore:#000000,bold,back:#FFB0FF
#Standard UDF's
style.au3.15=fore:#000000,back:#FFB0FF