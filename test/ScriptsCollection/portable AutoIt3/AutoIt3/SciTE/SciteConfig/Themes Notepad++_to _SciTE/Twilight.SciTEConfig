font.base=font:DejaVu Sans Mono,size:10,$(font.override)
font.monospace=font:DejaVu Sans Mono,size:10
proper.case=0
check.updates.scite4autoit3=0
use.tabs=1
indent.size=4
indent.size.*.au3=4
tabsize=4
#Background
style.au3.32=style.*.32=$(font.base),back:#141414
# Brace highlight
style.au3.34=fore:#FCE94F,back:#141414
# Brace incomplete highlight
style.au3.35=fore:#5F5A60,italics,back:#141414

# Line Number Margin - колонка номеров строк
style.au3.33=fore:#EEEEEC,back:#2E3436,$(font.base)
style.au3.37=fore:#EEEEEC,back:#2E3436
#line.margin.visible=1
line.margin.width=1+

#  олонка плюсиков дл€ разворачивани€ блоков
fold.margin.colour=#555753
fold.margin.highlight.colour=#2E3436
#fold.compact=0

# ћигающий курсор
caret.fore=#A7A7A7
caret.line.back=#292929
#caret.width=2
#caret.line.back.alpha=100

# ¬ыделенна€ область курсором
selection.fore=#8F9D6A
selection.back=#3E3E3E
# selection.alpha=75

# ¬ертикальна€ лини€ справа, edge.mode=0 убирает эту линию, edge.column - отступ слева до линии
edge.colour=#EEEEEC
edge.mode=0
#edge.column=500

#Console - запись в консоль при выполнении скрипта по F5
style.errorlist.32=back:#141414,$(font.monospace)
style.errorlist.0=fore:#8F9D6A
style.errorlist.5=fore:#000000
style.errorlist.3=fore:#CDA869
style.errorlist.4=fore:#DAD085
style.errorlist.11=fore:#5F5A60
style.errorlist.12=fore:#000000
error.marker.fore=fore:#ff0000,italics,back:#141414
colour.error=fore:#ff0000,back:#CDA869
#White space
style.au3.0=fore:#F8F8F8,back:#141414
#Comment line
style.au3.1=fore:#5F5A60,italics,back:#141414
#Comment block
style.au3.2=fore:#5F5A60,italics,back:#141414
#Number
style.au3.3=fore:#CF6A4C,back:#141414
#Function
style.au3.4=fore:#DAD085,back:#141414
#Keyword
style.au3.5=fore:#F9EE98,back:#141414
#Macro
style.au3.6=fore:#000000,back:#141414
#String
style.au3.7=fore:#8F9D6A,back:#141414
#Operator
style.au3.8=fore:#CDA869,back:#141414
#Variable
style.au3.9=fore:#7587A6,back:#141414
#Sent keys
style.au3.10=fore:#000000,bold,back:#141414
#Pre-Processor
style.au3.11=fore:#8996A8,back:#141414
#Special
style.au3.12=fore:#000000,back:#141414
#Abbrev-Expand
style.au3.13=fore:#000000,bold,back:#141414
#Com Objects
style.au3.14=fore:#000000,bold,back:#141414
#Standard UDF's
style.au3.15=fore:#F8F8F8,back:#141414